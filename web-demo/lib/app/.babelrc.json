{
  "plugins": [
    "babel-plugin-add-module-exports",
    "@babel/plugin-proposal-class-properties",
    "babel-plugin-lodash"
  ],
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "env": {
    "development": {
      "plugins": ["react-refresh/babel"]
    },
    "test": {
      "presets": [["@babel/preset-env", { "targets": { "node": "current" } }]]
    }
  }
}
